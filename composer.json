{
    "name": "forme/codegen",
    "license": "MIT",
    "description": "Code Generation CLI For The Forme WordPress Framework",
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0",
        "nette/php-generator": "^3.4",
        "forme/framework": "^1.3",
        "nikic/php-parser": "^4.9",
        "symfony/yaml": "^6.0",
        "consolidation/comments": "^1.0",
        "symfony/string": "^6.0",
        "jawira/case-converter": "^3.4",
        "symfony/process": "^5.1",
        "nette/utils": "^3.2",
        "league/flysystem": "^2.2",
        "php-di/php-di": "^6.3",
        "phppkg/cli-markdown": "dev-main#3e1c2e2b"
    },
    "autoload": {
        "psr-4": {
            "Forme\\CodeGen\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "psy/psysh": "^0.11.2",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "nunomaduro/mock-final-classes": "^1.1"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit tests --colors"
    },
    "bin": [
        "forme"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
